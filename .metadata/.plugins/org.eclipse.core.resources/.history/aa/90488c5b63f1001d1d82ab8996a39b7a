
public class Array04 {

	public static void main(String[] args) {
		
		int [] arr = {1,0,2,3,0,4,5,0};
		
		duplicateZeros(arr);

		for(int a : arr) {
			System.out.println(a);
		}
		
	}
	
    public static void duplicateZeros(int[] arr) {
        	
    	for(int i = 0;i<8;i++) {
    		
    		if(arr[i]==0) {
    			
    			arr[i+1] = arr[i];
 
    		}

    		
    	}
    	
    }

}
